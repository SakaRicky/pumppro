{
	"name": "e-store-pro-server",
	"version": "1.0.0",
	"description": "server for the e-store-pro build to manage store",
	"main": "index.js",
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"scripts": {
		"tsc": "tsc",
		"migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev --name postgres-init",
		"migrate:dev:reset": "dotenv -e .env.dev -- npx prisma migrate reset",
		"migrate:local": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
		"migrate:test:reset": "dotenv -e .env.test -- npx prisma migrate reset",
		"migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
		"migrate:prod-reset": "dotenv -e .env.prod -- npx prisma migrate reset",
		"db_push_remote": "dotenv -e .env -- npx prisma db push",
		"db_push_local": "dotenv -e .env.dev -- npx prisma db push",
		"db_push_test": "dotenv -e .env.test -- npx prisma db push",
		"db_seed_local": "cross-env NODE_ENV=dev dotenv -e .env.dev -- npx prisma db seed",
		"db_seed_test": "cross-env NODE_ENV=test dotenv -e .env.test -- npx prisma db seed",
		"db_seed_prod": "cross-env NODE_ENV=prod dotenv -e .env.prod -- npx prisma db seed",
		"dev": "cross-env NODE_ENV=development ts-node-dev --exit-child src/index.ts",
		"local_dev": "cross-env NODE_ENV=development ts-node-dev --exit-child src/index.ts",
		"local_test": "cross-env NODE_ENV=test ts-node-dev --exit-child src/index.ts",
		"start": "ts-node src/index.ts",
		"lint": "eslint .",
		"test": "cross-env NODE_ENV=test tsx --test src/tests/index.ts",
		"test_one_file": "cross-env NODE_ENV=test tsx --test src/tests/productSales.test.ts",
		"studio_remote": "dotenv -e .env -- npx prisma studio",
		"studio_dev": "dotenv -e .env.dev -- npx prisma studio",
		"studio_test": "dotenv -e .env.test -- npx prisma studio"
	},
	"author": "Ricky Saka",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@eslint/js": "^9.25.1",
		"@flydotio/dockerfile": "^0.5.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.15",
		"@types/jest": "^29.2.6",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"babel-cli": "^6.26.0",
		"babel-jest": "^29.4.0",
		"babel-preset-env": "^1.7.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.1",
		"globals": "^16.0.0",
		"jest": "^29.3.1",
		"prisma": "^5.20.0",
		"superagent": "^8.0.8",
		"supertest": "^6.3.3",
		"ts-node-dev": "^2.0.0",
		"tsx": "^4.19.2",
		"typescript": "^4.9.5",
		"typescript-eslint": "^8.31.1"
	},
	"dependencies": {
		"@prisma/client": "^5.20.0",
		"@types/bcrypt": "^5.0.0",
		"@types/express-session": "^1.17.5",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/multer": "^1.4.7",
		"@types/supertest": "^2.0.12",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.1",
		"cloudinary": "^1.41.1",
		"cors": "^2.8.5",
		"datauri": "^4.1.0",
		"dotenv": "^16.0.3",
		"dotenv-cli": "^7.0.0",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"express-session": "^1.17.3",
		"jsonwebtoken": "^9.0.0",
		"multer": "^1.4.5-lts.1",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"zod": "^3.20.2"
	}
}
